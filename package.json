{
  "name": "@serversinc/sdk",
  "version": "1.3.0",
  "description": "Official TypeScript SDK for the Serversinc API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "files": [
    "dist",
    "src/types.d.ts"
  ],
  "scripts": {
    "build": "npm run lint && tsc && npm run minify",
    "lint": "tsc --noEmit",
    "minify": "find dist -name '*.js' -exec terser {} -o {} --compress --mangle \\;",
    "postbuild": "find dist/resources -name '*.d.ts' -delete",
    "generate-types": "openapi-typescript ../path/to/openapi.yaml --output src/types.d.ts"
  },
  "keywords": [
    "api",
    "sdk",
    "serversinc",
    "typescript",
    "docker",
    "deployment"
  ],
  "author": "Serversinc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/serversinc/sdk"
  },
  "devDependencies": {
    "openapi-typescript": "^7.0.0",
    "terser": "^5.44.1",
    "typescript": "^5.0.0"
  }
}
